{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev_docs": {
      "dependsOn": ["dev_web"],
      "cache": false
    },
    "dev_web": {
      "cache": false
    }
  }
}
